<div class="container-xxl">
  <h4>Lista de eletrodos cadastrados:</h4>
      <a href="/admin/cadastro/eletrodo" class="btn btn-primary btn-md mb-2">Novo</a>
    </div>
    <div class="table-responsive">
      <table class="table table-bordered table-striped table-highlight">
        <thead>
          <th>idEl</th>
          <th>ECODIGO</th>
          <th>EDESCRIÇÃO</th>
        <tbody>
          {{#if getElAl}}
          {{#each getElAl}}
          <tr>

            <td>{{idEl}}</td>
            <td>{{ECODIGO}} </td>
            <td>{{EDESCRICAO}}</td>
             <td><a href="#"  class="btn btn-danger me-2 mb-2"> <i class="bi bi-x-circle-fill"></i></a><a href="#" class="btn btn-info mb-2"><i class="bi bi-pencil-square"></i></a></td>

          </tr>
          {{/each}}
          {{else}}
          <p class="d-flex align-items-center">Não há eletrodos cadastrados</p>
          {{/if}}
    </div>
    </tbody>
    </table>
</div>
